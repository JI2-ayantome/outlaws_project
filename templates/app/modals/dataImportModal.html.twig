<!-- Modal -->
<div class="modal fade" id="dataImportModal" data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="staticBackdropLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="staticBackdropLabel">Importation de donn√©es</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form {# action = "{{path('app_data_import_action')}}" method = "POST" #}>
                    <div class="row my-3">
                        <div class="custom-file">
                            <input type="file" class="custom-file-input" id="excelFileInput" name = "excel_file" required>
                            <label class="custom-file-label" for="validatedCustomFile">Choose file...</label>
                            <div class="invalid-feedback mb-3">Example invalid custom file feedback</div>
                        </div>
                    </div>

                    <div class="progress d-none mt-4">
                        <div class="progress-bar progress-bar-striped" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%"></div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary disabled" id = "btnImport">Importer</button>
            </div>
        </div>
    </div>
</div>